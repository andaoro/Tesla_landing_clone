---
import LandingHeader from "../components/LandingHeader.astro";
import HomeSection from "../components/HomeSection.astro";
import Layout from "../layouts/Layout.astro";
import ModelY from "../components/ModelY.astro";
import Model3 from "../components/Model3.astro";
import ModelS from "../components/ModelS.astro";
import ModelX from "../components/ModelX.astro";
import PowerWall from "../components/PowerWall.astro";
import Accesories from "../components/Accesories.astro";
---

<Layout title="Tesla landing Clone">
  <LandingHeader />

  <main
    class="snap-y snap-mandatory overflow-x-hidden relative w-full h-screen overflow-auto"
  >
    <div class="snap-center">
      <HomeSection />
    </div>
    <div class="snap-center">
      <ModelY/>
    </div>
    <div class="snap-center">
      <Model3/>
    </div>
    <div class="snap-center">
      <ModelS/>
    </div>
    <div class="snap-center">
      <ModelX/>
    </div>
    <div class="snap-center">
      <PowerWall/>
    </div>
    <div class="snap-center">
      <Accesories/>
    </div>
  </main>
</Layout>
